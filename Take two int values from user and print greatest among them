// Take two int values from user and print greatest among them.

importjava.util.Scanner;
publicclassFindGreatest{
publicstaticvoidmain(Stringargs[]){
intfirstN,secondN,thirdN;
Stringres;
Scannerin=newScanner(System.in);
System.out.print("EnterFirstNumber");
firstN=Integer.parseInt(in.nextLine());
System.out.print("EntersecondNumber");
secondN=Integer.parseInt(in.nextLine());
System.out.print("EnterthirdNumber");
thirdN=Integer.parseInt(in.nextLine());
if(firstN>secondN&&firstN>thirdN)
{
res=+firstN+"isgreatest.";
}
elseif(secondN>firstN&&secondN>thirdN)
{
res=+secondN+"isgreatest.";
}
elseif(thirdN>firstN&&thirdN>secondN)
{
res=+thirdN+"isgreatest";
}
else
res="Allareequal.";

System.out.println(res);
}
}
